#include <stdio.h>
#include <math.h>


  float a,b,x,prx,c;
  void main()
  {
     printf ("Ievadi sakuma vertibu: ");
  scanf ("%f", &a);
    printf ("Ievadi beigu vertibu: ");
  scanf ("%f", &b);
    printf ("Ievadi precizitati: ");
  scanf ("%f", &prx);
  printf("\n 1=skaitliskaa; 2=analitiskaa formula\n");
  printf("ievadi vajadzigas darbibas skaitli: ");
  scanf("%f",&c);

  if (c=1)
    {


      printf("\tx\t\t cos(x)\t\tcos\'(x)\n");
      x = a;
      while(x<b)
      {
         printf("%10.2f\t%10.2f\t%10.2f\n",x,cos(x),(cos(x+prx)-cos(x))/prx);

         x+= prx;
      }
    }

  if (c=2)
    {



      printf("\tx\t\t cos(x)\t\tcos\'(x)\n");
      x=a;
      while (x<b)
      {
          printf("%10.2f\t%10.2f\t%10.2f\n",x,cos(x),(cos(x)-cos(x-prx))/prx);
          x+=prx;
      }
    }

  }
